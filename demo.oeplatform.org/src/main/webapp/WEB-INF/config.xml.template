<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Config>
	<Encoding>ISO-8859-1</Encoding>
	<StyleSheets>
		<StyleSheet language="Swedish" default="true" name="default (xsl/design.xsl)" fullMenu="true">xsl/design.xsl</StyleSheet>
	</StyleSheets>
	<SystemXMLDebug>false</SystemXMLDebug>
	<SystemXMLDebugFile>system_xmldebug.xml</SystemXMLDebugFile>
	<ModuleXMLDebug>false</ModuleXMLDebug>
	<ModuleXMLDebugFile>module_xmldebug.xml</ModuleXMLDebugFile>
	<BackgroundModuleXMLDebug>false</BackgroundModuleXMLDebug>
	<BackgroundModuleXMLDebugFile>bgmodule_xmldebug.xml</BackgroundModuleXMLDebugFile>	
	<CoreDAOFactory>se.unlogic.hierarchy.core.daos.implementations.mysql.MySQLCoreDAOFactory</CoreDAOFactory>
	<DataSource>
		<Url>jdbc:mysql://host:3306/db</Url>
		<Type>SystemManaged</Type>
		<Driver>com.mysql.jdbc.Driver</Driver>
		<Username>user</Username>
		<Password>password</Password>
		<LogAbandoned>false</LogAbandoned>
		<RemoveAbandoned>false</RemoveAbandoned>
		<RemoveAbandonedTimeout>30</RemoveAbandonedTimeout>
		<TestOnBorrow>false</TestOnBorrow>
		<ValidationQuery>SELECT 1</ValidationQuery>
		<MaxActive>10</MaxActive>
		<MaxIdle>5</MaxIdle>
		<MinIdle>0</MinIdle>
		<MaxWait>0</MaxWait>
	</DataSource>
	<DefaultLanguage>Swedish</DefaultLanguage>
</Config>
