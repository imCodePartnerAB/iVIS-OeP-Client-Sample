            "Lantmateriet:fastighet_granser"
         ],
         "format":"image/png",
         "styles":[
            ""
         ],
         "customParams":{
            "TRANSPARENT":true
         }
      },
      {
         "type":"Vector",
         "styles":{
            "Point": {
                "pointRadius": 30,
                "externalGraphic": "http://sundsvall.e-tjansteportalen.se/mapclient-riges/images/point_cross.png"
            }
            $labelStyles
         },
         "styleProperty":"_style",
         "geoJson":{
            "type":"FeatureCollection",
            "features": $features
         },
         "name":"Drawings",
         "opacity":1
