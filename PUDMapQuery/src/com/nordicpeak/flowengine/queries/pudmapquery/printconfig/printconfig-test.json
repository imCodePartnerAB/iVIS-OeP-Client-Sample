{
   "units":"m",
   "outputFormat":"png",
   "layout":"$layout",
   "srs":"$srs",
   "dpi":$dpi,
   "attribution": "Kartan har ingen rättsverkan du kan alltså INTE se exakta gränser i denna karttjänst ©Lantmäteriet Geodatasamverkan",
   "layers":[
      {
         "baseURL":"http://extmaptest.sundsvall.se/geoserver/wms",
         "opacity":1,
         "singleTile":true,
         "type":"WMS",
         "layers":[
            "$baseLayer"
         ],
         "format":"image/png",
         "styles":[
            ""
         ]
      },
      {
         "baseURL":"http://extmaptest.sundsvall.se/geoserver/wms",
         "opacity":1,
         "singleTile":true,
         "type":"WMS",
         "layers":[
            "Lantmateriet:fastighet_granser"
         ],
         "format":"image/png",
         "styles":[
            ""
         ],
         "customParams":{
            "TRANSPARENT":true
         }
      },
      {
         "type":"Vector",
         "styles":{
            "Point": {
                "pointRadius": 10,
                "graphicName": "circle",
                "fillColor": "#29bf4c",
                "fillOpacity": 1,
                "strokeWidth": 1,
                "strokeOpacity": 1,
                "strokeColor": "#aeaeae"
            }
            $labelStyles
         },
         "styleProperty":"_style",
         "geoJson":{
            "type":"FeatureCollection",
            "features": $features
         },
         "name":"Drawings",
         "opacity":1
      }
   ],
   "pages":[
      {
         "center": $center,
         "scale": $scale,
         "rotation": 0
      }
   ]
}